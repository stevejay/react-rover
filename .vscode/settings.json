{
  "search.exclude": {
    ".vite/": true,
    ".yarn/": true,
    "coverage/": true,
    "dist/": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.markdownlint": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "env-*": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "env*": true,
    "tsconfig.tsbuildinfo": true,
    ".vite/": true
  },
  "css.validate": false,
  "stylelint.enable": true
}
